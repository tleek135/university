#include <iostream>

using namespace std;

int main(){
	int n = 0;
	
	while (n != 1023){
		cout << "What is the number? ";
		cin >> n;

		if (n < 1023)
			cout << "Too small!\n";
		else if (n > 1023)
			cout << "Too big!\n";
		else
			cout << "You got it!\n";
	}
}
